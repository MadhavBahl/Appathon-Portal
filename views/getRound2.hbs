<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="description" content="ADG: Apple Developers Group
    is a free initiative to assist developers in higher education
    institution (VIT), who intend to teach development for the
    iPhone, iPad and iPod Touch.This student body allows
    developers to create teams of up to 200 members who can
    develop, test, debug and distribute applications via the
    App Store giving them complete access to developer
    tools, including iPhone SDK and
    iPhone Dev Centre resources." />
    <title>App-A-Thon 2k18</title>
    
    <link rel="shortcut icon" href="/img/adg-logo.png">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="/css/materialize.min.css"  media="screen,projection"/>
    <link rel="stylesheet" href="/css/getrev.css">
</head>
<body>

    <div class="header">
        <div class="container">
            <div class="row" style="padding-top: 28px;">
                <div class="col s12 m5">
                    <img src="/img/appathon.png" class="responsive-img" alt="">
                </div>

                <div class="col s12 m5 offset-m1 hide-on-small-only">
                    <h2 style="text-align: right;">Marking Portal</h2>
                </div>
            </div>
        </div>
    </div>

    <div class="mainCard" id="mainCard1">
        <div class="container mainCardContainer">
            <div class="row">
                <div class="col s12 m12">
                    <div class="card">
                        <div class="card-content innerCard">
                            <div class="row">
                                <div class="col s12 m6">
                                    <h1 class="step"><b>Internal Portal</b></h1>
                                    <h5 class="step between"><b>Fetch Data For Each Team</b></h5>
                                </div>

                                <div class="col m4 offset-m2 hide-on-small-only" style="text-align: right;">
                                    <img src="/img/fullicon.webp" class="responsive-img ninety" alt="">
                                </div>
                            </div>

                            <div class="row">

                                {{#each data}}

                                    {{!-- <div class="col s12 m3">
                                        <div class="card hoverable">
                                            <div class="card-content cardPad">
                                                
                                                <p>TEAM {{this.num}}</p>
                                                <h5 class="center-align fixFont">{{this.teamName}}</h5>
                                                <p class="hide done{{this.num}}">{{this.done}}</p>
                                                <form action="{{this.judge}}/review/{{this.num}}" class="sendForm" method="POST">
                                                    <div class="right-align">
                                                        <button id="{{this.id}}" class="btn waves-effect waves-light btn-small padLess" type="submit" name="action">Submit</button>
                                                    </div>
                                                </form>

                                            </div>
                                        </div>
                                    </div> --}}

                                    <ul class="collapsible">
                                        <li>
                                            <div class="collapsible-header">
                                            
                                                {{this.teamNum}}. {{this.teamName}}
                                            {{!-- <span class="new badge"></span> --}}
                                            </div>
                                            <div class="collapsible-body">
                                                <p> <b>Team Name: </b> {{this.teamName}} </p>
                                                <p> <b>Team Number: </b> {{this.teamNum}}  </p>
                                                <p> <b>Team Participants: <br> </b> 
                                                    {{#each this.participant}}
                                                        <b>Name: </b> {{this.name}}  <br>
                                                        <b>Register Number: </b> {{this.rollNo}}  <br>
                                                    {{/each}}
                                                
                                                </p>

                                                <p id="done{{this.teamNum}}">{{this.done}}</p>
                                                <p> <b>Team Product Name: </b> {{this.productName}} </p>
                                                <p> <b>Team Product Description: </b> {{this.description}} </p>

                                                <form action="/saveForFinal/{{this.teamNum}}" class="sendForm" method="POST">
                                                    <div class="right-align">
                                                        <button id="{{this.teamNum}}" class="btn waves-effect waves-light btn-small padLess" type="submit" name="action">Submit</button>
                                                    </div>
                                                </form>
                                            </div>
                                        </li>
                                        
                                    </ul>

                                {{/each}}

                                <div class="col s12 m12 center">
                                    <h1 >Total Count: <span id="count">{{count}} </span></h1>
                                </div>

                            </div>

                        </div>
                    </div>

                </div>
            </div>

            
        </div>
    </div>


    <div class="footer">
        <div class="container">
            
        </div>
    </div>

    <!--Import jQuery before materialize.js-->
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="/js/materialize.min.js"></script>
    <script type="text/javascript" src="/js/activeBTN.js"></script>
</body>
</html>